<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="com.project.GameForm">
  <grid id="27dc6" binding="GamePanel" layout-manager="BorderLayout" hgap="0" vgap="0">
    <constraints>
      <xy x="20" y="20" width="500" height="400"/>
    </constraints>
    <properties/>
    <border type="none"/>
    <children>
      <grid id="3868b" binding="HeaderPanel" layout-manager="BorderLayout" hgap="0" vgap="0">
        <constraints border-constraint="North"/>
        <properties/>
        <border type="none"/>
        <children>
          <component id="6590d" class="javax.swing.JLabel" binding="TitleLabel">
            <constraints border-constraint="Center"/>
            <properties>
              <font name="Courier New" size="18"/>
              <horizontalAlignment value="0"/>
              <horizontalTextPosition value="0"/>
              <text value="Guessing Number Game"/>
            </properties>
          </component>
          <hspacer id="8f319">
            <constraints border-constraint="North"/>
          </hspacer>
        </children>
      </grid>
      <grid id="d6d0e" binding="ContentPanel" layout-manager="GridLayoutManager" row-count="1" column-count="2" same-size-horizontally="false" same-size-vertically="false" hgap="-1" vgap="-1">
        <margin top="0" left="0" bottom="0" right="0"/>
        <constraints border-constraint="Center"/>
        <properties/>
        <border type="none"/>
        <children>
          <component id="51b0" class="javax.swing.JLabel" binding="UserInputLabel">
            <constraints>
              <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
            </constraints>
            <properties>
              <text value="Input your guess : "/>
            </properties>
          </component>
          <component id="70f18" class="javax.swing.JTextField" binding="UserInputField">
            <constraints>
              <grid row="0" column="1" row-span="1" col-span="1" vsize-policy="0" hsize-policy="6" anchor="8" fill="1" indent="0" use-parent-layout="false">
                <preferred-size width="150" height="-1"/>
              </grid>
            </constraints>
            <properties/>
          </component>
        </children>
      </grid>
      <grid id="c5dcd" binding="FooterPanel" layout-manager="BorderLayout" hgap="0" vgap="0">
        <constraints border-constraint="South"/>
        <properties/>
        <border type="none"/>
        <children>
          <grid id="e61dc" binding="SubmitBtnPanel" layout-manager="BorderLayout" hgap="0" vgap="0">
            <constraints border-constraint="North"/>
            <properties/>
            <border type="none"/>
            <children>
              <component id="87160" class="javax.swing.JButton" binding="SubmitBtn">
                <constraints border-constraint="Center"/>
                <properties>
                  <font size="14" style="1"/>
                  <text value="Submit"/>
                </properties>
              </component>
              <component id="942ea" class="javax.swing.JLabel" binding="MessageLabel">
                <constraints border-constraint="South"/>
                <properties>
                  <horizontalAlignment value="0"/>
                  <horizontalTextPosition value="0"/>
                  <text value="Enter a number above and click submit."/>
                </properties>
              </component>
            </children>
          </grid>
          <component id="b0405" class="javax.swing.JLabel" binding="CopyrightLabel">
            <constraints border-constraint="Center"/>
            <properties>
              <horizontalAlignment value="0"/>
              <horizontalTextPosition value="0"/>
              <text value="Copyright 2024 Made by Joko"/>
            </properties>
          </component>
          <hspacer id="77c2b">
            <constraints border-constraint="South"/>
          </hspacer>
        </children>
      </grid>
      <vspacer id="8aff8">
        <constraints border-constraint="West"/>
      </vspacer>
      <vspacer id="1fbe6">
        <constraints border-constraint="East"/>
      </vspacer>
    </children>
  </grid>
</form>
