<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="com.game.GameForm">
  <grid id="27dc6" binding="GamePanel" layout-manager="BorderLayout" hgap="0" vgap="0">
    <constraints>
      <xy x="20" y="20" width="500" height="400"/>
    </constraints>
    <properties/>
    <border type="none"/>
    <children>
      <grid id="ff33a" binding="HeaderPanel" layout-manager="BorderLayout" hgap="0" vgap="0">
        <constraints border-constraint="North"/>
        <properties/>
        <border type="none"/>
        <children>
          <component id="1626b" class="javax.swing.JLabel" binding="TitleLabel">
            <constraints border-constraint="Center"/>
            <properties>
              <font name="Inconsolata" size="20" style="1"/>
              <horizontalAlignment value="0"/>
              <horizontalTextPosition value="0"/>
              <text value="Guessing Number Game"/>
            </properties>
          </component>
        </children>
      </grid>
      <grid id="9aad2" binding="ContentPanel" layout-manager="GridLayoutManager" row-count="1" column-count="2" same-size-horizontally="false" same-size-vertically="false" hgap="-1" vgap="-1">
        <margin top="0" left="0" bottom="0" right="0"/>
        <constraints border-constraint="Center"/>
        <properties/>
        <border type="none"/>
        <children>
          <component id="bd82a" class="javax.swing.JLabel" binding="UserInputLabel">
            <constraints>
              <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
            </constraints>
            <properties>
              <text value="Input Number"/>
            </properties>
          </component>
          <component id="52aed" class="javax.swing.JTextField" binding="UserInputField">
            <constraints>
              <grid row="0" column="1" row-span="1" col-span="1" vsize-policy="0" hsize-policy="6" anchor="8" fill="1" indent="0" use-parent-layout="false">
                <preferred-size width="150" height="-1"/>
              </grid>
            </constraints>
            <properties/>
          </component>
        </children>
      </grid>
      <grid id="2715d" binding="FooterPanel" layout-manager="BorderLayout" hgap="0" vgap="0">
        <constraints border-constraint="South"/>
        <properties/>
        <border type="none"/>
        <children>
          <component id="a6279" class="javax.swing.JLabel" binding="MessageLabel">
            <constraints border-constraint="North"/>
            <properties>
              <horizontalAlignment value="0"/>
              <horizontalTextPosition value="0"/>
              <text value="Input Your Number and Click Button to Submit"/>
            </properties>
          </component>
          <component id="c8be" class="javax.swing.JButton" binding="SubmitButton">
            <constraints border-constraint="Center"/>
            <properties>
              <text value="Button"/>
            </properties>
          </component>
          <component id="ec28" class="javax.swing.JLabel" binding="CopyrightLabel">
            <constraints border-constraint="South"/>
            <properties>
              <horizontalAlignment value="0"/>
              <horizontalTextPosition value="0"/>
              <text value="Copyright 2024 - made by Joko"/>
            </properties>
          </component>
        </children>
      </grid>
    </children>
  </grid>
</form>
